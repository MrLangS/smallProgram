Page{
  background: #282923 !important;
}
.content {
  width: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  z-index: 2;
}

.logo{
  width: 200rpx;
  height: 200rpx;
}

.admin-box{
  width: 85%;
  height: 300rpx;
  display: flex;
  flex-direction: row;
  justify-content: space-around;
  align-items: center;
  border-radius: 10px;
  box-shadow: 0 0 10px 0 rgba(0, 0, 0, 0.05);
  margin-top: 50rpx;
  background: #3281ff;
  color: white;
}

.staff-box{
  width: 85%;
  height: 300rpx;
  display: flex;
  flex-direction: row;
  justify-content: space-around;
  align-items: center;
  border-radius: 10px;
  box-shadow: 0 0 10px 0 rgba(0, 0, 0, 0.05);
  margin-top: 50rpx;
  background: #36c8c3;
  color: white;
}

.box-title view{
  margin: 5rpx;
}

.box-title{
  display: flex;
  width: 50%;
  flex-direction: column;
  justify-content: space-around;
  overflow: hidden;
}