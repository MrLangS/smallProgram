  transition: all 0.4s;
  -webkit-transform: translateX(90px);
  transform: translateX(90px);
  margin-left: -90px;
  overflow: hidden
}
.pro {
  /* width: 100%; */
  background: white;
  height: 180rpx;
  display: flex;
  justify-content: space-between;
  align-items: center; 
  /* padding-left: 40rpx; */
